<div class="container container__form">
  <ul>
  <li>
    <% current_account.account_wishlist.each do |wishlist| %>
      <%= link_to image_tag(wishlist.item.image, style:"width:50%"), wishlist.item %> <%= link_to wishlist.item.title, wishlist.item %> - <%= number_to_currency wishlist.item.price %>
    <% end %>
  </li>
  </ul>
</div>